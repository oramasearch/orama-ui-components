{
  "compilerOptions": {
    "target": "es2019",
    "lib": ["es2019", "dom"],
    "types": ["cypress", "@testing-library/cypress", "node"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"]
}
